/launch TYPE=sequencer INSTANCE=SEQUENCER

/SEQUENCER/start URL="stream://${APPLICATION}" STREAM-ID="${APPLICATION}" TIME-FREQUENCY=1
/SEQUENCER/addServer NAME="DOWNSTREAM_MULTICAST" URL="${DOWNSTREAM_MULTICAST}"
/SEQUENCER/addServer NAME="UPSTREAM_MULTICAST" URL="${UPSTREAM_MULTICAST}"
/SEQUENCER/addServer NAME="SEQUENCER_REWIND" URL="beam://0.0.0.0:18000?discoveryId=sequencer-rewind"
